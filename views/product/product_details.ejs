    .text-input:focus+.City-name::after {
      content: "*";
    }

    .text-input:focus+.house-number::after {
      content: "*";
    }

    .text-input:focus+.area-sector::after {
      content: "*";
    }

    .address-check {
      margin-top: 8px;
    }

    .address-check input {
      accent-color: #3e68af;
      width: 12px;
      height: 12px;
      transform: scale(1.5);
    }

    .address-check label {
      margin-left: 8px;
      font-size: 16px;
    }

    .contactFooter {
      background-color: white;
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 15px 20px;
      border-bottom-left-radius: 20px;
      border-bottom-right-radius: 20px;
    }

    .contactFooter .btnPrice {
