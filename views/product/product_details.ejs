  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    /* The Modal (background) */
    #checkoutModal {
      /* display: none;  */
      position: fixed;
      /* Stay in place */
      z-index: 10000;
      /* Sit on top */
      left: 0;
      top: 0px;
      width: 100%;
      /* Full width */
      height: 100%;
      /* Full height */
      overflow: auto;
      /* Enable scroll if needed */
      background-color: rgb(0, 0, 0);
      /* Fallback color */
      background-color: rgba(0, 0, 0, 0.4);
      /* Black w/ opacity */
    }

    /* Modal Content/Box */
    .modal-contentCheckout {
      /* background: linear-gradient(#146fb6, #633693); */
      /* background-image: url("../../front_assets/img/checkout_1.png"); */
      background-color: #146fb6;
      /* background-size: cover; */
      margin: 10% auto;
      padding: 20px;
      border-radius: 20px;
      width: 60%;
      height: 500px;
      /* position: relative;  */
    }

    /* The Close Button */
    .checkoutClose {
      color: #aaa;
      float: right;
      font-size: 28px;
      font-weight: bold;
    }

    .checkoutClose:hover,
    .checkoutClose:focus {
      color: black;
      text-decoration: none;
      cursor: pointer;
    }

    .backContent {
      width: 100%;
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      grid-template-rows: 1fr;
    }

    .contactContainer {
      background-color: white;
      margin-top: -82px;
      border-radius: 17px;
    }

    .contactContainer .contactHead {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 10px 18px;
      /* background: linear-gradient(#146fb6, #633693); */
      background: #146fb6;
      color: white;
      border-top-left-radius: 17px;
      border-top-right-radius: 17px;
    }

    .contactContainer .contactHead .headTitle {
      font-size: 25px;
      font-weight: 600;
    }

    .contactBody {
      padding: 20px;
      background-color: #ececec;
      height: 500px;
      overflow-y: scroll;
    }

    .contactBody .bodyHead {
      /* padding: 20px; */
      display: flex;
      align-items: center;
    }

    .bodyHead .userIcon {
      background-color: #f4f3f8;
      padding: 2px;
      border-radius: 50%;
      font-size: 25px;
    }

    .bodyHead .text {
      padding-left: 5px;
      font-weight: 600;
    }

    /* css for phone number input field  */
    .phone-container {
      width: 100%;
      position: relative;
    }

    .phone-container .lavel {
      position: absolute;
      left: 10px;
      top: 7px;
      transition: all 0.2s;
      padding: 5px 2px 0 80px;
      z-index: 1;
      color: #b3b3b3;
    }

    .phone-container .text-input {
      /* padding: 0.8rem; */
      padding: 16px 16px 16px 80px;
      width: 100%;
      height: 50px;
      border: 2px solid #146FB6;
      /* border: none; */
      background-color: white;
      border-radius: 5px;
      font-size: 18px;
      outline: none;
      transition: all 0.3s;
      color: #000000;
    }

    .phone-container .lavel::before {
      content: "";
      height: 3px;
      /* background-color: #e90909; */
      background-color: white;
      position: absolute;
      left: 80px;
      top: 18px;
      width: 54%;
      z-index: -1;
    }

    /* .phone-container .text-input:focus {
        border: 2px solid #9b9c9d;
      } */

    .phone-container .text-input:focus+.lavel,
    .field {
      top: -18px;
      color: #345094;
      font-size: 14px;
    }

    .phone-container .text-input:valid+.lavel,
    .field {
      top: -18px;
    }

    .text-input::placeholder {
      font-size: 16px;
      opacity: 0;
      transition: all 0.3s;
    }

    .text-input:focus::placeholder {
      opacity: 1;
      animation-delay: 0.2s;
    }

    /* css for country code dropdown menue  */

    .setCodeDropdown {
      position: absolute;
      top: -32px;
      left: -32px;
      z-index: 2;
    }

    .phoneDropdown {
      width: 50%;
      height: 50px;
      position: relative;
      margin: 2em;
      /* height: 100%; */
    }

    .phoneDropdown * {
      box-sizing: border-box;
    }

    .countrySelect {
      height: 100%;
      background: white;
      color: #9b9c9d;
      display: flex;
      justify-content: space-between;
      align-items: center;
      /* border: 2px solid #9b9c9d; */
      border: 2px solid #146FB6;
      /* border: none; */
      /* border-bottom: none; */
      border-top-left-radius: 0.5em;
      border-bottom-left-radius: 0.5em;
      padding: 1em;
      cursor: pointer;
      transition: background 0.3s;
    }

    /* added later in javaScript */
    .select-clicked {
      border: 2px solid #26489a;
      box-shadow: 0 0 0.8em #26489a;
    }

    .countrySelect:hover {
      background: #f2f2f2;
      color: #9b9c9d;
    }

    .countryCaret {
      width: 0;
      height: 0;
      margin: 0 10px;
      border-left: 5px solid transparent;
      border-right: 5px solid transparent;
      border-top: 6px solid #9b9c9d;
      transition: 0.3s;
    }

    /* added later in javaScript */
    .caret-rotate {
      transform: rotate(180deg);
    }

    .countryMenu {
      list-style: none;
      padding: 0.2em 0.5em;
      background: #f2f2f2;
      border: 1px solid #363a43;
      box-shadow: 0 0.5em 1em rgba(0, 0, 0, 0.2);
      border-radius: 0.5em;
      color: black;
      position: absolute;
      top: 55px;
      left: 50%;
      width: 100%;
      transform: translateX(-50%);
      opacity: 0;
      display: none;
      transition: 0.2s;
      z-index: 60000000;
    }

    .countryMenu li {
      padding: 0.7em 0.5em;
      margin: 0.5em 0;
      border-radius: 0.5em;
      cursor: pointer;
      z-index: 1;
    }

    .countryMenu li:hover {
      background: #a6a6a8;
    }

    /* added later in javaScript */
    .active {
      /* background: #a6a6a8; */
      color: black;
    }

    /* added later in javaScript */
    .menu-open {
      display: block;
      opacity: 1;
    }

    .input-container {
      margin: 10px 0;
      position: relative;
    }

    .lavel {
      position: absolute;
      left: 10px;
      top: 14px;
      transition: all 0.2s;
      padding: 0 2px;
      z-index: 1;
      color: #9b9c9d;
    }

    .text-input {
      padding: 0.8rem;
      width: 100%;
      height: 100%;
      border: 2px solid #146FB6;
      background-color: white;
      border-radius: 5px;
      font-size: 18px;
      outline: none;
      transition: all 0.3s;
      color: #000;
    }

    .lavel::before {
      content: "";
      height: 5px;
      background-color: white;
      position: absolute;
      left: 0;
      top: 10px;
      width: 100%;
      z-index: -1;
    }

    /* .text-input:focus {
        border: 2px solid #345094;
      } */
    .text-input:focus {
      border: 2px solid #345094;
    }



    .text-input:focus+.lavel,
    .field {
      top: -10px;
      color: #345094;
      font-size: 14px;
    }

    .text-input:valid+label,
