  } */
    .text-input::placeholder {
      font-size: 16px;
      opacity: 0;
      transition: all 0.3s;
    }

    .text-input:focus::placeholder {
      opacity: 1;
      animation-delay: 0.2s;
    }

    .text-input:focus+.full-name::after {
      content: "*";
    }

    .input_dropdown {
      display: grid;
      /* grid-template-columns: repeat(2, 1fr); */
      grid-template-rows: 1fr;
    }

    /* css for dropdown menue  */
    /* css for dropdown menue  */
    .checkoutDropdown {
