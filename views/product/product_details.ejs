      /* background: linear-gradient(#146fb6, #633693); */
      background: #146fb6;
      border: none;
      /* border: 1px solid black; */
      border-radius: 10px;
      padding: 8px 30px;
      font-size: 20px;
      font-weight: 600;
      letter-spacing: 1px;
      color: white;
    }

    @media only screen and (max-width: 1300px) {

      /* CSS rules for large screens */
      .modal-contentCheckout {
        width: 70%;
      }
    }

    @media only screen and (max-width: 1150px) {

      /* CSS rules for large screens */
      .modal-contentCheckout {
        width: 85%;
        margin: 15% auto;
      }
    }

    @media only screen and (max-width: 800px) {
      .modal-contentCheckout {
        width: 90%;
        margin: 20% auto;
      }
    }

    @media only screen and (max-width: 800px) {
      .modal-contentCheckout {
        width: 90%;
        margin: 20% auto;
      }
    }

    @media only screen and (max-width: 750px) {
      .modal-contentCheckout {
        overflow-y: scroll;
        padding: 10px;
      }

      .backContent {
        display: block;
      }

      .contactBody {
        padding: 10px;
      }

      .contactContainer {
        margin-top: 0;
      }

      .contactBody {
        overflow-y: visible;
        height: 630px;
      }

      .address-check label {
        margin-left: 8px;
        font-size: 15px;
      }

      .contactContainer .contactHead {
        background: linear-gradient(#633693, #146fb6);
      }

      .setCodeDropdown {
        top: -27px;
        left: -27px;
      }

      .contactFooter {
        padding: 15px 10px;
      }

      .contactFooter .ProceedBtn {
        padding: 5px 20px;
      }

    }
  </style>
